package io.asnetty.handler.logging {

/**
 * Maps the regular <code>LogLevel</code>s with the
 * <code>InternalLogLevel</code> ones.
 *
 * @author Jeremy
 */
public final class LogLevel {

    static public const TRACE:int = 0;
    static public const DEBUG:int = 1;
    static public const INFO:int  = 2;
    static public const WARN:int  = 3;
    static public const ERROR:int = 4;

} // class LogLevel
}
